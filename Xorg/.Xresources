Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.autohint: 0
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.dpi: 96
URxvt*interalBorder: 4
URxvt.font:xft:Hack:style=Regular:size=9
URxvt.boldfont:xft:Hack:style=Bold:size=9
URxvt.italicFont:xft:Hack:style=Italic:size=9
URxvt.boldItalicFont:xft:Hack:style=Bold Oblique:size=9
URxvt.scrollBar:false
URxvt*iconFile: /home/canopy/Downloads/App-Terminal-icon.png
URxvt.letterSpace:-1
