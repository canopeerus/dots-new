Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.autohint: 0
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.dpi: 96
URxvt*interalBorder: 4
URxvt.font:xft:Hack:style=Regular:size=9
URxvt.boldfont:xft:Hack:style=Bold:size=9
